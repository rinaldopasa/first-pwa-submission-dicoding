{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "eslint: recommended"
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-undef": "off",
    "no-console": "off",
    "no-restricted-globals": "off"
    // "no-useless-return": "off"
  }
}
